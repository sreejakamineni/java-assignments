package com.company;

import java.util.logging.Level;
import java.util.logging.Logger;
class Alpha
{
    /* This function check wether the given string contains all the alphabets.


     * @param s the input string taken.
     *Input : a string s.
     *Output: True or False.
     */


    public void function(String s)
    {
        Logger LOGGER =
                Logger.getLogger(Logger.GLOBAL_LOGGER_NAME);

        int val,t;
        int count =0;
        for(int i=0;i<s.length();i++)
        {
            val= s.indexOf(s.charAt(i)); //here val stores the index value where it first occured
            t= s.indexOf(val,s.charAt(i));// it checks for the requried chareter index after val index
            if(t<val) // checking if it is unique
            {
                count++;
            }

        }
        if(count==26)  // if the count is 26 it means all the alphabets are present
        {
            LOGGER.log(Level.INFO,"true");
        }
        else
        {
            LOGGER.log(Level.INFO,"false");
        }


    }
}


public class Main {

    public static void main(String[] args) {
        // write your code here
        Alpha a = new Alpha();
        a.function("abcdefghijklmnopqrstuvwxyz");
    }
}


